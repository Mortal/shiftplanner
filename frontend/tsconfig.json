{
	"compilerOptions": {
		"strict": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"allowJs": false,
		"module": "esnext",
		"sourceMap": true,
		"jsx": "react",
		"experimentalDecorators": true,
		"target": "esnext",
		"moduleResolution": "node",
		"downlevelIteration": true,
		"lib": [
			"es6",
			"dom"
		]
	}
}
